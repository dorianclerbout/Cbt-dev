                    <div class="col-md-12 col-sm-12">
                         <div class="home-info">
                              <h1>Nous sommes presque prêts !</h1>

                              
                              <!-- You can change the date time in init.js file -->
                              <ul class="countdown">
                                   <li>
                                        <span class="days">14</span>
                                        <h3>Jours</h3>
                                   </li>
                                   <li>
                                        <span class="hours">10</span>
                                        <h3>heures</h3>
                                   </li>
                                  
                              </ul>
                              <div class="subscribe-form">
                               
                              </div>
                         </div>
                    </div>

               </div>
          </div>
